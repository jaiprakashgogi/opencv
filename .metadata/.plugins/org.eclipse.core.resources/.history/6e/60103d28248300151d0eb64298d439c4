#include <iostream>
#include "opencv2/highgui.hpp"
#include "opencv2/imgproc.hpp"

using namespace std;
using namespace cv;

Mat detectSurf(Mat& img){

}

int main() {
	VideoCapture cap;
	char key = 'a';
	if (!cap.open(0))
		return 0;
	while (key != 'q') {
		Mat frame;
		cap >> frame;
		imshow("window", frame);
		Mat surf = detectSurf(frame);
		key = waitKey(1);
		cout << key <<endl;
	}
	return 0;
}
